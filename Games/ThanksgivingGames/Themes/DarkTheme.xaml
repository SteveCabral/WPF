<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#333"/>
    <SolidColorBrush x:Key="ButtonBrush" Color="#444"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="ListViewBrush" Color="#252526"/>
    <SolidColorBrush x:Key="PanelBrush" Color="#2C2C2C"/>
    <SolidColorBrush x:Key="TextBlockBackgroundBrush" Color="#2D2D30"/>
    <SolidColorBrush x:Key="TextBlockForegroundBrush" Color="#FFD700"/>
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="#2D2D30"/>
    <SolidColorBrush x:Key="TextBoxForegroundBrush" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="TextBrush" Color="#EAEAEA"/>
</ResourceDictionary>
